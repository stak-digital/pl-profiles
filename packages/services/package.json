{
	"name": "@pl-profiles/services",
	"version": "1.0.0",
	"scripts": {
		"lambda-serve": "netlify-lambda serve lambda-src",
		"lambda-build": "netlify-lambda build lambda-src",
		"build": "NODE_ENV=production webpack && yarn run lambda-build",
		"start": "NODE_ENV=development concurrently \"webpack-dev-server --content-base src/\" \"yarn run lambda-serve\""
	},
	"dependencies": {
		"netlify-lambda": "^1.6.3"
	},
	"devDependencies": {
		"concurrently": "^5.2.0",
		"dotenv": "^8.2.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.11.0"
	}
}
